{
  "name": "playlister-web",
  "description": "The Web Version of Playlister",
  "author": "Nick Derevjanik",
  "license": "GPL-3.0-only",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf coverage/ dist/ out/",
    "format": "prettier . --check",
    "lint": "eslint .",
    "compile": "tsc",
    "jest": "jest --coverage --verbose",
    "test": "npm-run-all clean format lint compile jest",
    "dist": "rollup --config",
    "version": "sed -i \"s/{version}/${SEMVER:-Development}/g\" dist/playlister.user.js",
    "build": "npm-run-all test dist version",
    "build:local": "npm-run-all test dist",
    "watch": "tsc --watch"
  },
  "devDependencies": {
    "@eslint/js": "^8.56.0",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.0",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "ts-jest": "^29.1.5",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.11.0"
  }
}
